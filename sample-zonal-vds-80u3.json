{
    "control_plane": {
        "login_banner": "You are connect to a multi-az Supervisor",
        "network": {
            "backing": {
                "backing": "NETWORK_SEGMENT",
                "network_segment": {
                    "networks": [ "${TKGMgmtNetwork1}","${TKGMgmtNetwork2}","${TKGMgmtNetwork3}" ]
                }
            },
            "ip_management": {
                "dhcp_enabled": false,
                "gateway_address": "192.168.100.1/23",
                "ip_assignments": [ {
                    "assignee": "NODE",
                    "ranges": [ {
                        "address": "192.168.100.60",
                        "count": 5
                    } ]
                } ]
            },
            "network": "mgmt",
            "proxy": {
                "proxy_settings_source": "VC_INHERITED"
            },
            "services": {
                "dns": {
                    "search_domains": [ "env1.lab.test" ],
                    "servers": [ "192.168.100.1" ]
                },
                "ntp": {
                    "servers": [ "time1.vmware.com" ]
                }
            }
        },
        "size": "SMALL",
        "storage_policy": "${TKGStoragePolicy}"
    },
    "name": "multi-az-supervisor0",
    "workloads": {
        "edge": {
            "nsx_advanced": {
                "certificate_authority_chain": ${AVI_CACERT},
                "cloud_name": "${AVI_CLOUD}",
                "password": "${AVI_PASSWORD}",
                "server": {
                    "host": "${AVI_HOSTNAME}",
                    "port": 443
                },
                "username": "admin"
            },
            "provider": "NSX_ADVANCED"
        },
        "images": {
           "kubernetes_content_library": "${TKGContentLibrary}"
        },
        "network": {
            "ip_management": {
                "dhcp_enabled": false,
                "gateway_address": "192.168.102.1/23",
                "ip_assignments": [ {
                    "assignee": "NODE",
                    "ranges": [ {
                        "address": "192.168.102.10",
                        "count": 100
                    } ]
                },
                {
                    "assignee": "SERVICE",
                    "ranges": [ {
                        "address": "10.96.0.0",
                        "count": 512
                    } ]
                } ]
            },
            "network": "wkld0",
            "services": {
                "dns": {
                    "search_domains": [ "env1.lab.test" ],
                    "servers": [ "192.168.100.1" ]
                },
                "ntp": {
                    "servers": [ "time1.vmware.com" ]
                }
            },
            "network_type": "VSPHERE",
            "vsphere": {
                "dvpg": "${TKGWorkload0Network}"
            }
        },
        "storage": {
            "ephemeral_storage_policy": "${TKGStoragePolicy}",
            "image_storage_policy": "${TKGStoragePolicy}"
        }
    },
    "zones": [ "${TKGZone1}","${TKGZone2}","${TKGZone3}" ]
}